#include<stdio.h>
main()
{
	int choice=0;
	printf("enter your choice:\n1.burger,Rs 129\n2,french fries,Rs 99\n3.pasta,Rs 179\n4.sandwich,Rs 149\n5.pizza,Rs 239");
	scanf("%d",&choice);
	
	switch(choice)
	{
		case 1: printf("food item-burger \n price-Rs 129");
		break;
		case 2: printf("food item-french fries \n price-Rs 99");
		break;
		case 3: printf("food item-pasta \n price-Rs 179");
		break;
		case 4: printf("food item-sandwich \n price- Rs 149");
		break;
		case 5: printf("food item-pizza \n price- Rs 239");
		break;
		default: printf(" number is not in the list");
	}
	
	
	
	
	
}
